<img class="phone" src="{{ phone.images.[0] }}">

<h1>{{{ phone.name }}}</h1>

<p>{{{ phone.description }}}</p>

<ul class="phone-thumbs">

  {{#each phone.images as |image|}}

    <li>
      <img src="{{ image }}">
    </li>

  {{/each}}

</ul>

<button data-element="back-button">Go back</button>
<button data-element="add-button">Add to basket</button>
